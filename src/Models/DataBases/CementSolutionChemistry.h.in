template<typename T>
_INLINE_ T (CementSolutionChemistry_ComputeLiquidMassDensity)(CementSolutionChemistry_t<T>* csc)
/** Return the liquid mass density **/
{
  T c_h        = Concentration(H) ;
  T c_oh       = Concentration(OH) ;
  
  T c_ca       = Concentration(Ca) ;
  T c_caoh     = Concentration(CaOH) ;
  T c_caoh2    = Concentration(CaO2H2) ;
  
  T c_h4sio4   = Concentration(H4SiO4) ;
  T c_h3sio4   = Concentration(H3SiO4) ;
  T c_h2sio4   = Concentration(H2SiO4) ;
  
  T c_na       = Concentration(Na) ;
  T c_naoh     = Concentration(NaOH) ;
  
  T c_k        = Concentration(K) ;
  T c_koh      = Concentration(KOH) ;
  
  T c_cl       = Concentration(Cl) ;
  
  T c_co2      = Concentration(CO2) ;
  T c_h2co3    = Concentration(H2CO3) ;
  T c_hco3     = Concentration(HCO3) ;
  T c_co3      = Concentration(CO3) ;
  
  T c_h2so4    = Concentration(H2SO4) ;
  T c_hso4     = Concentration(HSO4) ;
  T c_so4      = Concentration(SO4) ;
  
  T c_al       = Concentration(Al) ;
  T c_alo4h4   = Concentration(AlO4H4) ;
  
  T c_caso4    = Concentration(CaSO4) ;
  T c_cahso4   = Concentration(CaHSO4) ;
  
  T c_cah2sio4 = Concentration(CaH2SiO4) ;
  T c_cah3sio4 = Concentration(CaH3SiO4) ;
  
  T c_naco3    = Concentration(NaCO3) ;
  T c_nahco3   = Concentration(NaHCO3) ;
  
  T c_caco3    = Concentration(CaCO3) ;
  T c_cahco3   = Concentration(CaHCO3) ;
  
  
  /* Partial molar volumes */
  T v_h      = c_h*V(H) + c_oh*V(OH) ;
  T v_ca     = c_ca*V(Ca) + c_caoh*V(CaOH) + c_caoh2*V(CaO2H2) ;
  T v_si     = c_h3sio4*V(H3SiO4) + c_h4sio4*V(H4SiO4) + c_h2sio4*V(H2SiO4) ;
  T v_na     = c_na*V(Na) + c_naoh*V(NaOH) ;
  T v_k      = c_k*V(K) + c_koh*V(KOH) ;
  T v_cl     = c_cl*V(Cl) ;
  T v_c      = c_co2*V(CO2) + c_h2co3*V(H2CO3) + c_hco3*V(HCO3) + c_co3*V(CO3) ;
  T v_s      = c_h2so4*V(H2SO4) + c_hso4*V(HSO4) + c_so4*V(SO4) ;
  T v_al     = c_al*V(Al) + c_alo4h4*V(AlO4H4) ;
  T v_ca_si  = c_cah2sio4*V(CaH2SiO4) + c_cah3sio4*V(CaH3SiO4) ;
  T v_ca_c   = c_cahco3*V(CaHCO3) + c_caco3*V(CaCO3) ;
  T v_na_c   = c_nahco3*V(NaHCO3) + c_naco3*V(NaCO3) ;
  T v_ca_s   = c_cahso4*V(CaHSO4) + c_caso4*V(CaSO4) ;
  
  T v_all    = v_h + v_ca + v_si + v_na + v_k + v_c + v_cl + v_s + v_al \
                  + v_ca_si + v_ca_c + v_na_c + v_ca_s ;
  
  /* Water */
  T c_h2o    = (1 - v_all)/V(H2O) ;
       
  /* Update */
  Concentration(H2O)  = c_h2o ;
         
  /* Solution */
  T rho_0     = M(H)*c_h + M(OH)*c_oh + M(H2O)*c_h2o ;
  T rho_ca    = M(Ca)*c_ca + M(CaOH)*c_caoh + M(CaO2H2)*c_caoh2 ;
  T rho_si    = M(H3SiO4)*c_h3sio4 + M(H4SiO4)*c_h4sio4 + M(H2SiO4)*c_h2sio4 ;
  T rho_na    = M(Na)*c_na + M(NaOH)*c_naoh ;
  T rho_k     = M(K)*c_k + M(KOH)*c_koh ;
  T rho_cl    = M(Cl)*c_cl ;
  T rho_c     = M(CO2)*c_co2 + M(H2CO3)*c_h2co3 + M(HCO3)*c_hco3 + M(CO3)*c_co3 ;
  T rho_s     = M(H2SO4)*c_h2so4 + M(HSO4)*c_hso4 + M(SO4)*c_so4 ;
  T rho_al    = M(Al)*c_al + M(AlO4H4)*c_alo4h4 ;
  T rho_ca_si = M(CaH2SiO4)*c_cah2sio4 + M(CaH3SiO4)*c_cah3sio4 ;
  T rho_ca_c  = M(CaHCO3)*c_cahco3 + M(CaCO3)*c_caco3 ;
  T rho_na_c  = M(NaHCO3)*c_nahco3 + M(NaCO3)*c_naco3 ;
  T rho_ca_s  = M(CaHSO4)*c_cahso4 + M(CaSO4)*c_caso4 ;
  
  T rho_l = rho_0 + rho_ca + rho_si + rho_na + rho_k + rho_cl + rho_c + rho_s + rho_al \
               + rho_ca_si + rho_ca_c + rho_na_c + rho_ca_s ;
       
  /* Backup */
  CementSolutionChemistry_SetLiquidMassDensity(csc,rho_l) ;


  return(rho_l) ;
}

