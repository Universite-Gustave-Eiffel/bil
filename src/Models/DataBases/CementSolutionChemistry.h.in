template<typename T>
_INLINE_ void (CementSolutionChemistry_Initialize)(CementSolutionChemistry_t<T>* csc)
{
  /* Concentrations */
  {
    int     n = CementSolutionChemistry_NbOfSpecies ;
    T* v = CementSolutionChemistry_GetConcentration(csc) ;
  
    {
      int i ;
      
      for(i = 0 ; i < n ; i++) {
        v[i] = 0. ;
      }
    }
  }
  
  /* Log of concentrations */
  {
    int     n = CementSolutionChemistry_NbOfSpecies ;
    T* v = CementSolutionChemistry_GetLogConcentration(csc) ;
  
    {
      int i ;
      
      for(i = 0 ; i < n ; i++) {
        v[i] = - 99 ;
      }
    }
  }
  
  /* Log of activities */
  {
    int     n = CementSolutionChemistry_NbOfSpecies ;
    T* v = CementSolutionChemistry_GetLogActivity(csc) ;
  
    {
      int i ;
      
      for(i = 0 ; i < n ; i++) {
        v[i] = - 99 ;
      }
    }
  }
  
  /* Element concentrations */
  {
    int     n = CementSolutionChemistry_NbOfElementConcentrations ;
    T* v = CementSolutionChemistry_GetElementConcentration(csc) ;
  
    {
      int i ;
      
      for(i = 0 ; i < n ; i++) {
        v[i] = 0. ;
      }
    }
  }
  
  /* Other variables */
  {
    int     n = CementSolutionChemistry_NbOfOtherVariables ;
    T* v = CementSolutionChemistry_GetOtherVariable(csc) ;
  
    {
      int i ;
      
      for(i = 0 ; i < n ; i++) {
        v[i] = 0. ;
      }
    }
  }
}





template<typename T>
_INLINE_ void (CementSolutionChemistry_UpdateElementConcentrations)(CementSolutionChemistry_t<T>* csc)
/** Update the element concentrations **/
{
  //T c_h        = Concentration(H) ;
  //T c_oh       = Concentration(OH) ;
  
  /* Calcium */
  T c_ca       = Concentration(Ca) ;
  T c_caoh     = Concentration(CaOH) ;
  T c_caoh2    = Concentration(CaO2H2) ;
  
  /* Silicon */
  T c_h4sio4   = Concentration(H4SiO4) ;
  T c_h3sio4   = Concentration(H3SiO4) ;
  T c_h2sio4   = Concentration(H2SiO4) ;
  
  /* Sodium */
  T c_na       = Concentration(Na) ;
  T c_naoh     = Concentration(NaOH) ;
  
  /* Potassium */
  T c_k        = Concentration(K) ;
  T c_koh      = Concentration(KOH) ;
  
  /* Carbon */
  T c_co2      = Concentration(CO2) ;
  T c_h2co3    = Concentration(H2CO3) ;
  T c_hco3     = Concentration(HCO3) ;
  T c_co3      = Concentration(CO3) ;
  
  /* Sulfur */
  T c_h2so4    = Concentration(H2SO4) ;
  T c_hso4     = Concentration(HSO4) ;
  T c_so4      = Concentration(SO4) ;
  
  /* Aluminium */
  T c_al       = Concentration(Al) ;
  T c_alo4h4   = Concentration(AlO4H4) ;
  
  /* Chlore */
  T c_cl       = Concentration(Cl) ;
  
  /* Compounds of type II */
  T c_cah2sio4 = Concentration(CaH2SiO4) ;
  T c_cah3sio4 = Concentration(CaH3SiO4) ;
  
  T c_naco3    = Concentration(NaCO3) ;
  T c_nahco3   = Concentration(NaHCO3) ;
  
  T c_caco3    = Concentration(CaCO3) ;
  T c_cahco3   = Concentration(CaHCO3) ;
  
  T c_caso4    = Concentration(CaSO4) ;
  T c_cahso4   = Concentration(CaHSO4) ;

  
  /* Concentration as element: C, Ca, Si ... */
  /* Compounds type I */
  T c_ca_l    = c_ca + c_caoh + c_caoh2 ;
  T c_si_l    = c_h2sio4 + c_h3sio4 + c_h4sio4 ;
  T c_na_l    = c_na + c_naoh ;
  T c_k_l     = c_k + c_koh ;
  T c_c_l     = c_co2 + c_h2co3 + c_hco3 + c_co3 ;
  T c_s_l     = c_h2so4 + c_hso4 + c_so4 ;
  T c_al_l    = c_al + c_alo4h4 ;
  T c_cl_l    = c_cl ;
  /* Compounds type II */
  T c_ca_si_l = c_cah2sio4 + c_cah3sio4 ;
  T c_ca_c_l  = c_cahco3 + c_caco3 ;
  T c_ca_s_l  = c_caso4 + c_cahso4 ;
  T c_na_c_l  = c_nahco3 + c_naco3 ;
  
  
  /* Backup */
  
  CementSolutionChemistry_SetElementConcentrationOf(csc,Ca,c_ca_l + c_ca_si_l + c_ca_c_l + c_ca_s_l) ;
  CementSolutionChemistry_SetElementConcentrationOf(csc,Si,c_si_l + c_ca_si_l) ;
  CementSolutionChemistry_SetElementConcentrationOf(csc,Na,c_na_l + c_na_c_l) ;
  CementSolutionChemistry_SetElementConcentrationOf(csc,K,c_k_l) ;
  CementSolutionChemistry_SetElementConcentrationOf(csc,C,c_c_l + c_ca_c_l + c_na_c_l) ;
  CementSolutionChemistry_SetElementConcentrationOf(csc,S,c_s_l + c_ca_s_l) ;
  CementSolutionChemistry_SetElementConcentrationOf(csc,Al,c_al_l) ;
  CementSolutionChemistry_SetElementConcentrationOf(csc,Cl,c_cl_l) ;
}



template<typename T>
_INLINE_ void (CementSolutionChemistry_TranslateConcentrationsIntoActivities)(CementSolutionChemistry_t<T>* csc)
{
  T logc0 = LogC0_ref ;
  int     n = CementSolutionChemistry_NbOfSpecies ;
  T* logc = CementSolutionChemistry_GetLogConcentration(csc) ;
  T* loga = CementSolutionChemistry_GetLogActivity(csc) ;
  
  {
    int i ;
      
    for(i = 0 ; i < n ; i++) {
      loga[i] = logc[i] - logc0 ;
    }
  }
}




template<typename T>
_INLINE_ T (poly4)(T a,T b,T c,T d,T e,T a_h,T a_oh)
/* Solve ax^4 + bx^3 + cx^2 + dx + e = 0 
 * for x in the range defined by x*a_h < 1 and a_oh/x < 1 (a_oh < x < 1/a_h)
 * because a_h and a_oh should be < 1 (ie 0 < pH < -logKw) 
 * The new values of a_h and a_oh are: x*a_h and a_oh/x.
 * Return the solution which is the closest to 1. */
{
  T x ;
  
  {
    T y[5] = {a,b,c,d,e} ;
    int n = Math_ComputePolynomialEquationRoots(y,4) ;
    int i ;
    
    x = y[0] ;
    for(i = 1 ; i < n ; i++) {
      T x1 = fabs(x - 1) ;
      T y1 = fabs(y[i] - 1) ;
      
      if(y1 < x1) x = y[i] ;
    }

    /* Too constraining
    if((x*a_h > 1) || (a_oh/x > 1)) {
      printf("\n") ;
      printf("n    = %d\n",n) ;
      printf("a,b,c,d,e = %e,%e,%e,%e,%e\n",a,b,c,d,e) ;
      printf("x    = %e\n",x) ;
      printf("a_h  = %e\n",a_h) ;
      printf("a_oh = %e\n",a_oh) ;
      // Raise an interrupt signal instead of exit
      Message_Warning("poly4: a_h = %e > 1 or a_oh = %e > 1!",x*a_h,a_oh/x) ;
      return(-1) ;
    }
    */
  }
  
  {
    T y[5] = {a,b,c,d,e} ;
    T tol = 1e-4*fabs(x) ;
    int k = Math_PolishPolynomialEquationRoot(y,4,&x,tol,20) ;
    
    if(k < 0) return(-1) ;
  }
  
  return(x) ;
}

