template<typename T>
_INLINE_ void (CementSolutionChemistry_SupplementSystemWith_Al2O3_LogQ_AH3)(CementSolutionChemistry_t<T>* csc)
{
  /* Supplement with Al2O3 */
  T logq_ah3   = Input(LogQ_AH3) ;
  

  /* Water */
  T loga_oh  = LogActivity(OH) ;
  
  
  /* Chemical reactions involving compounds of type I. */

  /* Aluminium compounds */
  T loga_al     = 0.5*logq_ah3 - 3*loga_oh ;
  T logk_alo4h4 = LogKeq(AlO4H4) ;
  T loga_alo4h4 = loga_al + 4*loga_oh - logk_alo4h4 ;
  
  
  /* Chemical reactions involving compounds of type II. */
  
  
  /* Backup activities */
  {
    LogActivity(Al)     = loga_al ;
    LogActivity(AlO4H4) = loga_alo4h4 ;
  }
  
  
  /* Backup concentrations */
  {
    /* Translate into concentrations providing ideality */
    T logc0             = LogC0_ref ;
    
    T logc_al           = loga_al      + logc0 ;
    T logc_alo4h4       = loga_alo4h4  + logc0 ;
    
    Concentration(Al)        = pow(10,logc_al) ;
    Concentration(AlO4H4)    = pow(10,logc_alo4h4) ;
  
    LogConcentration(Al)     = logc_al ;
    LogConcentration(AlO4H4) = logc_alo4h4 ;
  }
}





